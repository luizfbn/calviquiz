{
  "expo": {
    "name": "Calviquiz",
    "slug": "calviquiz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#5068A9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.luizfbn.calviquiz"
    },
    "android": {
      "package": "com.luizfbn.calviquiz",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#5068A9"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
